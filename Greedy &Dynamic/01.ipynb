    "t =  int(input())\n",
    "for _ in range(t):\n",
    "    n = int(input())\n",
    "    coins = [50000,10000,5000,1000,500,100]\n",
    "    coinnum = 0\n",
    "    for coin in coins:\n",
    "        coinnum += n//coin\n",
    "        n %= coin\n",
    "    print(coinnum)"
