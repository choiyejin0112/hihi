    "t = int(input())  \n",
    "for _ in range(t):\n",
    "    N, m = map(int, input().split())  \n",
    "    Matrix = [[0] * N for _ in range(N)]  # 0으로 초기화했음 \n",
    "\n",
    "    for _ in range(m):\n",
    "        u, v, c = map(int, input().split())  \n",
    "        Matrix[u][v] = c\n",
    "\n",
    "    for i in range(N):\n",
    "        print(*Matrix[i])"
   ]
